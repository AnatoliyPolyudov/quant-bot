# config.py

# Trading settings
SYMBOL = "BTC-USDT-SWAP"
TIMEFRAME = "1m"

# Data collection
WS_URL = "wss://ws.okx.com:8443/ws/v5/public"
CHANNELS = [
    "books",              # –°—Ç–∞–∫–∞–Ω L2
    "trades",             # –õ–µ–Ω—Ç–∞ —Å–¥–µ–ª–æ–∫
    "tickers"             # –¢–∏–∫–µ—Ä—ã (funding, OI)
]

# Feature settings
FEATURE_WINDOW = 50

# Model settings
